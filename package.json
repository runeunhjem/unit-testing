{
  "name": "unit-testing",
  "version": "0.0.2",
  "description": "Test of Unit Testing",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npm run test-unit",
    "test-unit": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/runeunhjem/unit-testing.git"
  },
  "keywords": [
    "Jest"
  ],
  "author": "Rune Unhjem <runeunhjem@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "cypress": "^13.3.2",
    "eslint": "^8.51.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^27.4.2",
    "jest": "^29.2.0",
    "jest-fetch-mock": "^3.0.3",
    "node-localstorage": "^3.0.5"
  },
  "eslint": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
      {
        "files": [
          "**/*.test.mjs",
          "**/*.test.js"
        ],
        "env": {
          "jest": true
        },
        "plugins": [
          "jest"
        ],
        "extends": [
          "plugin:jest/recommended"
        ],
        "rules": {
          "jest/prefer-expect-assertions": "off"
        }
      }
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {}
  }
}
